<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_setting"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.panda.littlesquirrel.activity.SettingActivity">
    <FrameLayout
        android:id="@+id/ll_top"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_marginLeft="13dp"
        android:layout_marginRight="13dp"
        android:layout_marginTop="13dp">
        <include layout="@layout/top" />
    </FrameLayout>
    <FrameLayout
        android:layout_marginTop="87dp"
        android:id="@+id/ll_mid"
        android:layout_width="match_parent"
        android:layout_height="398dp"
        android:layout_marginLeft="13dp"
        android:layout_marginRight="13dp">
        <include layout="@layout/set_type"/>
    </FrameLayout>
    <FrameLayout
        android:id="@+id/ll_buttom"
        android:layout_marginTop="498dp"
        android:layout_width="333dp"
        android:layout_height="127dp"
        android:layout_marginLeft="13dp"
        android:layout_marginRight="13dp">
        <include layout="@layout/bottom" />
    </FrameLayout>

    <com.panda.littlesquirrel.view.BackAndTimerView
        android:id="@+id/backAndTime"
        android:layout_width="match_parent"
        android:layout_height="29dp"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="97dp"  />
</RelativeLayout>
